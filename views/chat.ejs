<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Chat App</title>
  <link rel="icon" href="/img/favicon.png" />
  <link rel="stylesheet" href="/css/styles.css" />
</head>

<body>
  <div class="chat">
    <div id="sidebar" class="chat__sidebar">
      <h1>Chat App</h1>

    </div>
    <div class="chat__main">
      <div class="messages chat__messages">
        
      </div>
      <div class="user-location"></div>
      <div class="compose">
        <form id="message-form">
          <input type="text" placeholder="Write your message here..." class="textspace" required autocomplete="off">
        </form>
        <button class="send">Send</button>
        <button class="send-location">Send Location</button>
      </div>

    </div>
  </div>

  <!-- <button class="btn">Click Me</button> -->
  <script id="message-template" type="text/ejs">
      <div class="messages">
          <p></p>
      </div>
    </script>
  <script id="location-template" type="text/ejs">
      <div class="messages">
        <p></p>
      </div>
    </script>
    <script class="sidebar-template" type="text/ejs">
      
    </script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/qs/6.6.0/qs.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/js/index.js"></script>
</body>

</html>